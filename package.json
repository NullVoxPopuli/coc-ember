{
  "name": "coc-ember",
  "version": "1.0.16",
  "private": false,
  "author": "NullVoxPopuli",
  "license": "MIT",
  "description": "Ember language server extension for coc.nvim",
  "main": "lib/index.js",
  "keywords": [
    "nvim",
    "coc.nvim",
    "ember",
    "ember-cli",
    "ember-language-server"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "build:language-server": "cd node_modules/@emberwatch/ember-language-server && yarn",
    "build:bundle-language-server": "cp -r node_modules/@emberwatch/ember-language-server lib/",
    "lint:js": "eslint . --ext .ts",
    "prepublish": "npx npm-run-all clean build:js build:language-server build:bundle-language-server"
  },
  "files": [
    "package.json",
    "lib",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "coc": "^0.0.73"
  },
  "activationEvents": [
    "onLanguage:hbs",
    "onLanguage:html.handlebars",
    "onLanguage:handlebars",
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "contributes": {
    "configuration": {
      "title": "Ember",
      "properties": {}
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.12.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "coc.nvim": "^0.0.74",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4",
    "vscode-languageserver-protocol": "^3.14.1"
  },
  "dependencies": {
    "@emberwatch/ember-language-server": "github:lifeart/ember-language-server#a3abb22c15655d922eae47ceeec24164397d651a"
  },
  "volta": {
    "node": "10.16.0",
    "yarn": "1.17.3"
  }
}
